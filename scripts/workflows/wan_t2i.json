{
  "1": {
    "class_type": "WanVideoModelLoader",
    "_meta": {
      "title": "Load WAN T2V Model"
    },
    "inputs": {
      "model": "wan2.2_t2v_high_noise_14B_Q4_K_M.gguf",
      "base_precision": "bf16",
      "quantization": "disabled",
      "load_device": "offload_device",
      "attention": "sdpa"
    }
  },
  "2": {
    "class_type": "WanVideoVAELoader",
    "_meta": {
      "title": "Load VAE"
    },
    "inputs": {
      "model_name": "wan_2.1_vae.safetensors",
      "precision": "bf16"
    }
  },
  "3": {
    "class_type": "CLIPLoader",
    "_meta": {
      "title": "Load Text Encoder"
    },
    "inputs": {
      "clip_name": "umt5_xxl_fp8_e4m3fn_scaled.safetensors",
      "type": "wan"
    }
  },
  "4": {
    "class_type": "CLIPTextEncode",
    "_meta": {
      "title": "Positive Prompt"
    },
    "inputs": {
      "text": "{{PROMPT}}",
      "clip": ["3", 0]
    }
  },
  "5": {
    "class_type": "CLIPTextEncode",
    "_meta": {
      "title": "Negative Prompt"
    },
    "inputs": {
      "text": "blurry, low quality, distorted, deformed, static, poorly drawn, disfigured, ugly, worst quality",
      "clip": ["3", 0]
    }
  },
  "6": {
    "class_type": "WanVideoTextEmbedBridge",
    "_meta": {
      "title": "Text Embed Bridge"
    },
    "inputs": {
      "positive": ["4", 0],
      "negative": ["5", 0]
    }
  },
  "7": {
    "class_type": "WanVideoEmptyEmbeds",
    "_meta": {
      "title": "Empty Image Embeds (T2I)"
    },
    "inputs": {
      "width": 832,
      "height": 480,
      "num_frames": 1
    }
  },
  "8": {
    "class_type": "WanVideoSampler",
    "_meta": {
      "title": "WAN Sampler"
    },
    "inputs": {
      "model": ["1", 0],
      "image_embeds": ["7", 0],
      "text_embeds": ["6", 0],
      "steps": 20,
      "cfg": 5.0,
      "shift": 5.0,
      "seed": 0,
      "force_offload": true,
      "scheduler": "unipc",
      "riflex_freq_index": 0
    }
  },
  "9": {
    "class_type": "WanVideoDecode",
    "_meta": {
      "title": "Decode Image"
    },
    "inputs": {
      "vae": ["2", 0],
      "samples": ["8", 0],
      "enable_vae_tiling": true,
      "tile_x": 272,
      "tile_y": 272,
      "tile_stride_x": 144,
      "tile_stride_y": 128
    }
  },
  "10": {
    "class_type": "SaveImage",
    "_meta": {
      "title": "Save Image"
    },
    "inputs": {
      "filename_prefix": "wan_t2i",
      "images": ["9", 0]
    }
  }
}
