{
  "1": {
    "class_type": "LoadImage",
    "inputs": {
      "image": "{{INPUT_IMAGE}}"
    },
    "_meta": {
      "title": "Load Source Image"
    }
  },
  "2": {
    "class_type": "DWPreprocessor",
    "inputs": {
      "image": ["1", 0],
      "detect_hand": "enable",
      "detect_body": "enable",
      "detect_face": "disable",
      "resolution": 832,
      "bbox_detector": "yolox_l.onnx",
      "pose_estimator": "dw-ll_ucoco_384_bs5.torchscript.pt"
    },
    "_meta": {
      "title": "DWPose Skeleton Extraction"
    }
  },
  "3": {
    "class_type": "SaveImage",
    "inputs": {
      "images": ["2", 0],
      "filename_prefix": "skeleton"
    },
    "_meta": {
      "title": "Save Skeleton"
    }
  }
}
